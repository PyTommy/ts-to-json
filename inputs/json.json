{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"TodoStatus": {
			"title": "TodoStatus",
			"enum": ["DONE", "IN_PROGRESS", "IN_REVIEW", "TODO"],
			"type": "string"
		},
		"Priority": {
			"title": "Priority",
			"enum": ["high", "low", "medium"],
			"type": "string"
		},
		"Tag": {
			"type": "object",
			"additionalProperties": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string",
						"title": "name"
					},
					"color": {
						"type": "string",
						"title": "color"
					}
				},
				"required": ["color", "name"]
			}
		}
	}
}
